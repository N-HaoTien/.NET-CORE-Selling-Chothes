@model CheckoutViewModel
@{
    Layout = null;
}
<div class="col-lg-6">
    <input type="hidden" asp-for="UserId"/>
    <h4>Biiling Details</h4>
    <div class="row">
        <div class="col-lg-12">
            <label for="street">Email Address<span>*</span></label>
            <input readonly type="text" asp-for="Email" class="street-first">
        </div>
        <div class="col-lg-12">
            <label for="street">Full Name<span>*</span></label>
            <input type="text" asp-for="Name" class="street-first">
        </div>
        <div class="col-lg-12">
            <label for="street">Street Address<span>*</span></label>
            <input type="text" asp-for="Address" class="street-first">
        </div>
        <div class="col-lg-12">
            <label for="phone">Phone<span>*</span></label>
            <input type="text" asp-for="Phone" >
        </div>

    </div>
</div>